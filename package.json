{
  "name": "blocksuite",
  "version": "0.2.1",
  "description": "BlockSuite framework workspace",
  "scripts": {
    "serve": "pnpm --filter @blocksuite/store serve",
    "dev": "pnpm serve & pnpm --filter @blocksuite/playground dev",
    "lint": "eslint ./ --max-warnings=0",
    "test:e2e": "playwright test",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:headed": "playwright test --headed",
    "test:unit": "vitest --run",
    "format": "prettier --write packages tests",
    "build": "pnpm -r build",
    "build:playground": "pnpm --filter @blocksuite/playground build",
    "clean": "rm -rf dist packages/{blocks,editor,playground,shared,store}/dist",
    "bump": "pnpm --recursive exec pnpm version minor && pnpm version minor --git-tag-version false",
    "publish": "pnpm --recursive --filter=!@blocksuite/playground exec pnpm publish --no-git-checks"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "@playwright/test": "^1.25.2",
    "@types/node": "^18.7.16",
    "@types/quill": "^2.0.9",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "pretty-format": "^29.1.2",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  }
}
