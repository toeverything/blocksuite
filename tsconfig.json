{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "useDefineForClassFields": false,
    "experimentalDecorators": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "NodeNext",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "incremental": true
  },
  "files": [],
  "references": [
    {
      "path": "./packages/blocks"
    },
    {
      "path": "./packages/editor"
    },
    {
      "path": "./packages/playground"
    },
    {
      "path": "./packages/store"
    },
    {
      "path": "./tests"
    }
  ]
}
