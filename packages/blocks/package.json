{
  "name": "@blocksuite/blocks",
  "version": "0.3.1",
  "description": "Default BlockSuite editable blocks.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/phasor": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@tldraw/intersect": "^1.8.0",
    "autosize": "^5.0.2",
    "highlight.js": "^11.7.0",
    "hotkeys-js": "^3.10.1",
    "lit": "^2.5.0",
    "perfect-freehand": "^1.2.0",
    "quill": "^1.3.7",
    "quill-cursors": "^4.0.0"
  },
  "exports": {
    "./dist/*": "./dist/*",
    ".": "./src/index.ts",
    "./std": "./src/std.ts",
    "./models": "./src/models.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
