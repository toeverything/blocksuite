{
  "name": "@blocksuite/blocks",
  "version": "0.10.0",
  "description": "Default BlockSuite editable blocks.",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest --run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test": "pnpm test:unit && pnpm test:e2e"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/global": "workspace:*",
    "@blocksuite/lit": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@blocksuite/virgo": "workspace:*",
    "@floating-ui/dom": "^1.5.3",
    "@toeverything/theme": "^0.7.27",
    "@types/hast": "^3.0.3",
    "@types/mdast": "^4.0.2",
    "@types/webfontloader": "^1.6.36",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "file-type": "^16.5.4",
    "fractional-indexing": "^3.2.0",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "lit": "^3.0.2",
    "marked": "^4.3.0",
    "nanoid": "^5.0.3",
    "pdf-lib": "^1.17.1",
    "rehype-parse": "^9.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "shiki": "^0.14.5",
    "sortablejs": "^1.15.0",
    "unified": "^11.0.4",
    "webfontloader": "^1.6.28",
    "zod": "^3.22.4"
  },
  "exports": {
    "./dist/*": "./dist/*",
    ".": "./src/index.ts",
    "./models": "./src/models.ts",
    "./content-parser": "./src/content-parser.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      "./dist/*": "./dist/*",
      "./src/*": "./dist/*.js",
      ".": {
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./models": {
        "types": "./dist/models.d.ts",
        "default": "./dist/models.js"
      },
      "./content-parser": {
        "types": "./dist/content-parser.d.ts",
        "default": "./dist/content-parser.js"
      }
    }
  },
  "files": [
    "dist",
    "content-parser.d.ts",
    "content-parser.js",
    "models.d.ts",
    "models.js"
  ],
  "devDependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/lit": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@types/marked": "^4.3.1",
    "@types/sortablejs": "^1.15.4"
  }
}
