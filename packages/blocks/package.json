{
  "name": "@blocksuite/blocks",
  "version": "0.8.0",
  "description": "Default BlockSuite editable blocks.",
  "main": "src/index.ts",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest --run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test": "pnpm test:unit && pnpm test:e2e"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "peerDependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/lit": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@toeverything/theme": "^0.7.13",
    "yjs": "^13"
  },
  "dependencies": {
    "@blocksuite/global": "workspace:*",
    "@blocksuite/phasor": "workspace:*",
    "@blocksuite/virgo": "workspace:*",
    "@floating-ui/dom": "^1.5.1",
    "@types/webfontloader": "^1.6.35",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "file-type": "^16.5.4",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "lit": "^2.8.0",
    "marked": "^4.3.0",
    "pdf-lib": "^1.17.1",
    "shiki": "^0.14.3",
    "sortablejs": "^1.15.0",
    "turndown": "^7.1.2",
    "webfontloader": "^1.6.28",
    "zod": "^3.22.2"
  },
  "exports": {
    "./dist/*": "./dist/*",
    ".": "./src/index.ts",
    "./std": "./src/std.ts",
    "./models": "./src/models.ts",
    "./content-parser": "./src/content-parser.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      "./dist/*": "./dist/*",
      "./src/*": "./dist/*.js",
      ".": {
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./std": {
        "types": "./dist/std.d.ts",
        "default": "./dist/std.js"
      },
      "./models": {
        "types": "./dist/models.d.ts",
        "default": "./dist/models.js"
      },
      "./content-parser": {
        "types": "./dist/content-parser.d.ts",
        "default": "./dist/content-parser.js"
      }
    }
  },
  "devDependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/lit": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@toeverything/theme": "^0.7.13",
    "@types/autosize": "^4.0.1",
    "@types/marked": "^4.3.1",
    "@types/sortablejs": "^1.15.1",
    "@types/turndown": "^5.0.1",
    "yjs": "^13.6.7"
  }
}
