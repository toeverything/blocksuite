      "./dist/*": "./dist/*",
      "./src/*": "./dist/*.js",
      ".": "./dist/index.js",
      "./std": {
        "types": "./dist/std.d.ts",
        "default": "./dist/std.js"
      },
      "./models": {
        "types": "./dist/models.d.ts",
        "default": "./dist/models.js"
      }
    }
  },
  "devDependencies": {
    "@types/autosize": "^4.0.1"
  }
