{
  "name": "@blocksuite/sync",
  "version": "0.12.0",
  "description": "BlockSuite data synchronization engine abstraction and implementation.",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest --run",
    "test": "pnpm test:unit"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/global": "workspace:*",
    "y-protocols": "^1.0.6",
    "idb": "^8.0.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "yjs": "^13"
  },
  "exports": {
    ".": "./src/index.ts",
    "./impl/*": "./src/impl/*"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./impl/*": {
        "types": "./dist/impl/*.d.ts",
        "module": "./dist/impl/*.js",
        "import": "./dist/impl/*.js"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
