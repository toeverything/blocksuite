{
  "name": "@blocksuite/inline",
  "version": "0.15.0",
  "description": "A micro editor.",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "nx vite:test --run",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:unit:ui": "nx vite:test --ui",
    "test:e2e": "nx e2e",
    "test": "pnpm test:unit && pnpm test:e2e"
  },
  "keywords": [],
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ],
  "author": "toeverything",
  "license": "MPL-2.0",
  "devDependencies": {
    "lit": "^3.1.4",
    "yjs": "^13.6.18"
  },
  "peerDependencies": {
    "lit": "^3.1.1",
    "yjs": "^13.6.15"
  },
  "exports": {
    "./*": "./src/*.ts",
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      "./*": "./dist/*.js",
      ".": "./dist/index.js"
    }
  },
  "dependencies": {
    "@blocksuite/global": "workspace:*",
    "zod": "^3.23.8"
  }
}
