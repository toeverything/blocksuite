{
  "name": "@blocksuite/global",
  "version": "0.14.0",
  "types": "./index.d.ts",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "test:unit": "vitest --run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "build": "tsc"
  },
  "sideEffects": false,
  "exports": {
    ".": "./index.d.ts",
    "./utils": "./src/utils.ts",
    "./env": "./src/env/index.ts",
    "./exceptions": "./src/exceptions/index.ts"
  },
  "typesVersions": {
    "*": {
      "utils": [
        "dist/utils.d.ts"
      ],
      "env": [
        "dist/env/index.d.ts"
      ],
      "exceptions": [
        "dist/exceptions/index.d.ts"
      ]
    }
  },
  "author": "toeverything",
  "license": "MPL-2.0",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./index.d.ts",
      "./utils": "./dist/utils.js",
      "./env": "./dist/env/index.js",
      "./exceptions": "./dist/exceptions/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "index.d.ts",
    "!src/__tests__",
    "!dist/__tests__"
  ],
  "dependencies": {
    "lib0": "^0.2.94",
    "zod": "^3.23.8"
  }
}
