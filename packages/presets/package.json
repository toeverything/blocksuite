{
  "name": "@blocksuite/presets",
  "version": "0.16.0",
  "description": "Prebuilt BlockSuite editors and opt-in additional UI components.",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc --build --verbose",
    "test:unit": "nx vite:test --browser.headless --run",
    "test:debug": "nx vite:test"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/blocks": "workspace:*",
    "@blocksuite/global": "workspace:*",
    "@blocksuite/inline": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@dotlottie/player-component": "^2.7.12",
    "@fal-ai/serverless-client": "^0.13.0",
    "@floating-ui/dom": "^1.6.8",
    "@lit-labs/preact-signals": "^1.0.2",
    "@toeverything/theme": "^1.0.2",
    "lit": "^3.1.4",
    "openai": "^4.53.2",
    "zod": "^3.23.8"
  },
  "exports": {
    ".": "./src/index.ts",
    "./themes/*": "./themes/*"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./ai": "./dist/ai/index.js",
      "./themes/*": "./themes/*"
    }
  },
  "files": [
    "src",
    "dist",
    "themes",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
