{
  "name": "@blocksuite/presets",
  "version": "0.12.0",
  "description": "Prebuilt BlockSuite editors and opt-in additional UI components.",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "scripts": {
    "build": "tsc --build --verbose",
    "test": "vitest --browser.headless --run",
    "test:debug": "vitest"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@blocksuite/block-std": "workspace:*",
    "@blocksuite/blocks": "workspace:*",
    "@blocksuite/global": "workspace:*",
    "@blocksuite/inline": "workspace:*",
    "@blocksuite/lit": "workspace:*",
    "@blocksuite/store": "workspace:*",
    "@fal-ai/serverless-client": "^0.8.2",
    "@floating-ui/dom": "^1.5.4",
    "@toeverything/theme": "^0.7.27",
    "lit": "^3.1.1",
    "openai": "^4.25.0"
  },
  "exports": {
    ".": "./src/index.ts",
    "./themes/*": "./themes/*"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./themes/*": "./themes/*"
    }
  },
  "files": [
    "src",
    "dist",
    "themes",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
