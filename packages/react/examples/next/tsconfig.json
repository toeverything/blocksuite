{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "Node",
    "noEmit": false,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": false,
    "noImplicitOverride": false
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
