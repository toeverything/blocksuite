    "directory": "packages/affine/all"
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "test:unit": "nx vite:test --run --passWithNoTests",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:e2e": "playwright test"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
