      "./caption": {
        "types": "./dist/caption/index.d.ts",
        "import": "./dist/caption/index.js"
      },
      "./context-menu": {
        "types": "./dist/context-menu/index.d.ts",
        "import": "./dist/context-menu/index.js"
      },
      "./date-picker": {
        "types": "./dist/date-picker/index.d.ts",
        "import": "./dist/date-picker/index.js"
